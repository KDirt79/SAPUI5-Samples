<mvc:View
    height="100%"
    controllerName="fahmaih.samples.warehouse.controller.Home"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page title="{i18n>homePageTitle}" 
          class="sapUiResponsiveContentPadding"
          showHeader="true"
          enableScrolling="false">
		<TileContainer
			id="container"
			tiles="{/Transactions}">
			<StandardTile
				icon="{icon}"
				type="{type}"
				number="{number}"
				numberUnit="{numberUnit}"
				title="{title}"
				info="{info}"
				infoState="{infoState}"
				press="onPress" />
		</TileContainer>
    </Page>
</mvc:View>